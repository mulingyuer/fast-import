{
  "name": "fast-import",
  "displayName": "fast-import",
  "description": "fast-import extension",
  "publisher": "mulingyuer",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.102.0"
  },
  "license": "SEE LICENSE IN LICENSE",
  "icon": "public/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/mulingyuer/fast-import"
  },
  "bugs": {
    "url": "https://github.com/mulingyuer/fast-import/issues"
  },
  "categories": [
    "Other"
  ],
  "galleryBanner": {
    "color": "#3e3838",
    "theme": "dark"
  },
  "activationEvents": [
    "onLanguage"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fast-import.moveToBraces",
        "title": "快速定位 (Import/解构赋值)"
      }
    ],
    "keybindings": [
      {
        "command": "fast-import.moveToBraces",
        "key": "alt+i",
        "mac": "alt+i",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "build": "vsce package",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}